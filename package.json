{
  "name": "@zuplo/mcp-app-instructions",
  "version": "0.0.11",
  "description": "Provides instructions for adding or connecting various apps to an MCP server.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zuplo/mcp-app-instructions"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
